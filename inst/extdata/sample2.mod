$PROBLEM SAMPLE (2-COMPARTMENT MODEL)

$INPUT ID TIME CMT EVID DV AMT

$DATA data.csv

$SUBROUTINE ADVAN13

$MODEL
COMP=(CENTRAL)
COMP=(PERIPHERAL)

$PK
TVCL = THETA(1)
CL = TVCL * EXP(ETA(1))

V1WT = (WT - 70) * THETA(6)
IF (SEX.EQ.1) THEN
  V1SEX = THETA(7)
ELSE
  V1SEX = THETA(8)
ENDIF
V2COV = V1WT * V1SEX

TVV1 = THETA(2)
V1 = TVV1 * EXP(ETA(2))

TVQ = THETA(3)
Q = TVQ * EXP(ETA(3))

TVV2 = THETA(4)
V2 = TVV2 * EXP(ETA(4))

KE = CL / V1
K12 = Q / V1
K21 = Q / V2

$DES
IF (T.LE.4) THEN
  KE = 0
ENDIF

DADT(1) = - K12 * A(1) + K21 * A(2) - KE * A(1)
DADT(2) = K12 * A(1) - K21 * A(2)

$ERROR
IPRED = A(1) / V1

W = THETA(5)

Y = IPRED * (1 + W * EPS(1))

IWRES = (DV - IPRED) / (IPRED * W)

$THETA
(0, 0.1) ; CL [L/day]
(0, 1)   ; V1 [L]
(0, 0.1) ; Q  [L/day]
(0, 1)   ; V2 [L]
(0, 0.1) ; W  [-]
(0, 0.5) ; V1SEX1 [-]
(0, 0.5) ; V1SEX2 [-]
(0, 0.5) ; V1WT [-]

$OMEGA BLOCK(2)
0.1 ; CL
0.1 0.1 ; V1

$OMEGA
0.1 ; Q
0.1 ; V2

$SIGMA
1 FIX

$ESTIMATION

$COVARIANCE
